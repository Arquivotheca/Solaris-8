#include <sys/types.h>
#include <unistd.h>

void *malloc(size_t nbytes);
void *realloc(void *ptr, size_t size);
void *calloc(size_t num, size_t size);
void free(void *ptr);
void cfree(void *p, unsigned num, unsigned size);
struct mallinfo mallinfo(void);
int mallopt(int, int);
