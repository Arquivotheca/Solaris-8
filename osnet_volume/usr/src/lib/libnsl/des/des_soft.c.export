	0x79, 0x79, 0x7a, 0x7a, 0x7c, 0x7c, 0x7f, 0x7f,
};

/*
 * Add odd parity to low bit of 8 byte key
 */
void
des_setparity(p)
	char *p;
{
	int i;

	trace1(TR_des_setparity, 0);
	for (i = 0; i < 8; i++) {
		*p = partab[*p & 0x7f];
		p++;
	}
	trace1(TR_des_setparity, 1);
}

