/*
 * A corrected version of des_setparity (see bug 1149767).
 */
void
des_setparity_g(des_block *p)
{
	int i;

	trace1(TR_des_setparity_g, 0);
	for (i = 0; i < 8; i++) {
		(*p).c[i] = partab_g[(*p).c[i]];
	}
	trace1(TR_des_setparity_g, 1);
}

