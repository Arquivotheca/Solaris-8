#pragma ident   "@(#)llib-llpcls 1.1     98/07/10 SMI"

typedef struct CLASS {
	char   *name;		/* name of class (redundant) */
	char   **members;       /* members of class */
}			CLASS;

int		putclass ( char *, CLASS * );
int		delclass ( char * );

void		freeclass ( CLASS * );

